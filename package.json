    "babel-preset-es2015": "^6.18.0",
    "babel-preset-react": "^6.16.0",
    "babel-preset-stage-0": "^6.16.0",
    "plasmajs": "^0.1.6",
    "react": "^15.3.2",
    "react-dom": "^15.3.2"
  },
  "devDependencies": {
    "babel-loader": "^6.2.8",
    "gulp": "^3.9.1",
    "gulp-autoprefixer": "^3.1.1",
    "gulp-minify-css": "^1.2.4",
    "gulp-sass": "^2.3.2",
    "webpack": "^2.1.0-beta.25"
  }
