{
  "name": "plasmajs-starter-kit",
  "version": "0.0.1",
  "description": "The starter kit for making a web app in plasmaJS",
  "main": "server.jsx",
  "scripts": {
    "start": "plasmajs .",
    "start:watch": "nodemon -e jsx --exec plasmajs .",
    "build:js": "webpack",
    "build:js:watch": "webpack -w",
    "build:js:prod": "webpack -p",
    "build:css": "gulp buildSASS",
    "build:css:watch": "gulp",
    "test": "echo \"Error: no test specified\" && exit 1"
